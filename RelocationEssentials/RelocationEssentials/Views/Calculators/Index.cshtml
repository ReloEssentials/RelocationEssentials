<script type="text/javascript">
    var principal = "", interest = "", length = "", dp = "0", income = "", addPrincipal = "";

    $(function () {
        swapTab(1, 1, 1);
    });

    function swapTab(tab, content, right) {
        if (content == 1)
            set1();
        else if (content == 2)
            set2();
        else if (content == 3)
            set3();
        else if (content == 11)
            set11();
        else if (content == 12)
            set12();
        else if (content == 13)
            set13();
        for (var i = 1; i < 5; i++) {
            document.getElementById('tabs-' + i).style.display = 'none';
        }
        for (var i = 1; i < 14; i++) {
            document.getElementById('content-' + i).style.display = 'none';
            if(i < 11)
                document.getElementById('right-' + i).style.display = 'none';
        }
        document.getElementById('tabs-' + tab).style.display = 'block';
        document.getElementById('content-' + content).style.display = 'block';
        document.getElementById('right-' + right).style.display = 'block';
    }

    function set1() {
        document.getElementById('Principal1').value = principal;
        document.getElementById('DownPMT1').value = dp;
        document.getElementById('Length1').value = length;
        document.getElementById('Rate1').value = interest;
    }

    function set2() {
        document.getElementById('Income2').value = income;
        document.getElementById('DownPMT2').value = dp;
        document.getElementById('Length2').value = length;
        document.getElementById('Rate2').value = interest;
    }

    function set3() {
        document.getElementById('Principal3').value = principal;
        document.getElementById('DownPMT3').value = dp;
        document.getElementById('Length3').value = length;
        document.getElementById('Rate3').value = interest;
        document.getElementById('AddPrincipal3').value = addPrincipal;
    }

    function set11() {
        principal = document.getElementById('Principal1').value;
        dp = document.getElementById('DownPMT1').value;
        length = document.getElementById('Length1').value;
        interest = document.getElementById('Rate1').value;

        document.getElementById('PrincipalLabel1').innerHTML = "$" + addCommas(principal);
        document.getElementById('DPLabel1').innerHTML = "$" + addCommas(dp);
        document.getElementById('LengthLabel1').innerHTML = addCommas(length) + " years";
        document.getElementById('RateLabel1').innerHTML = interest + "%";

        var monthlyInterest = interest/1200;
        var numOfPayments = length * 12;
        var mortgage = (principal - dp) * (monthlyInterest / (1 - Math.pow(1 + monthlyInterest, -numOfPayments)));
        document.getElementById('PaymentLabel1').innerHTML = "$" + addCommas(Math.round(mortgage));

        document.getElementById('TotalMonthlyLabel1').innerHTML = "$" + addCommas((mortgage + 2200/12 + 1000/12).toFixed(0));

        document.getElementById('Income1').innerHTML = "$" + addCommas((((mortgage + 2200/12 + 1000/12) / .28) * 12).toFixed(0));
    }

    function set12() {
        income = document.getElementById('Income2').value;
        dp = document.getElementById('DownPMT2').value;
        length = document.getElementById('Length2').value;
        interest = document.getElementById('Rate2').value;

        document.getElementById('PrincipalLabel2').innerHTML = "$" + addCommas(income);
        document.getElementById('LengthLabel2').innerHTML = length + " years";
        document.getElementById('RateLabel2').innerHTML = interest + "%";
        document.getElementById('DPLabel2').innerHTML = "$" + addCommas(dp);

        var mortgage = income / 12 * .28;
        document.getElementById('PaymentLabel2').innerHTML = "$" + addCommas(Math.round(mortgage));

        document.getElementById('RETaxLabel2').innerHTML = "$2,200";
        document.getElementById('HomeINSLabel2').innerHTML = "1,000";

        var monthly = mortgage + 2200 / 12 + 1000 / 12;
        document.getElementById('TotalMonthlyLabel2').innerHTML = "$" + addCommas(monthly.toFixed(0));

        var monthlyInterest = interest / 1200;
        var numOfPayments = length * 12;
        var PMT = mortgage - 2200 / 12 - 1000 / 12;
        var prin = PMT / (monthlyInterest / (1 - (Math.pow((1 + monthlyInterest), -numOfPayments))));
        var price = prin + dp;
        document.getElementById('PriceLabel2').innerHTML = "$" + addCommas(Math.round(price));
    }

    function set13() {
        principal = document.getElementById('Principal3').value;
        dp = document.getElementById('DownPMT3').value;
        length = document.getElementById('Length3').value;
        interest = document.getElementById('Rate3').value;
        addPrincipal = document.getElementById('AddPrincipal3').value;

        document.getElementById('PrincipalLabel13').innerHTML = "$" + addCommas(principal);
        document.getElementById('DPLabel13').innerHTML = "$" + addCommas(dp);
        document.getElementById('LengthLabel13').innerHTML = length + " years";
        document.getElementById('RateLabel13').innerHTML = interest + "%";
        document.getElementById('AddPrincipalLabel13').innerHTML = "$" + addCommas(addPrincipal);

        var show = (document.getElementById('AmorList30').checked) ? 0 : (document.getElementById('AmorList31').checked) ? 1 : 2;

        var monthlyInterest = interest / 1200;
        var numOfPayments = length * 12;
        var monthlyPayment = (principal - dp) * (monthlyInterest / (1 - (Math.pow((1 + monthlyInterest), -numOfPayments))));
        var adjusted = monthlyPayment + Number(addPrincipal);

        var d = new Date();
        var MI = 0;
        var MP = 0;
        var YearTotalI = 0;
        var YearTotalP = 0;

        var adjustedPrincipal = principal - dp;

        var row = 0;

        var html = "";

        if (show == 1)
            html += "<table width=\"100%\"><tr style=\"background-color:blue; text:#FFC; text-align:center\"><th>Year</th><th>Total Interest</th><th>Total Principal</th><th>Remaining Balance</th></tr>";

        while (adjustedPrincipal > .01) {
            MI = adjustedPrincipal * monthlyInterest;
            if ((adjustedPrincipal + MI) > adjusted) {
                MP = adjusted - MI;
            }
            else {
                //Last payment
                adjusted = adjustedPrincipal + MI;
                MP = adjustedPrincipal;
            }
            adjustedPrincipal -= MP;
            if (d.getMonth() == 11) {
                d.setMonth(0, 1);
                d.setFullYear(d.getFullYear() + 1);
            }
            else
                d.setMonth(d.getMonth() + 1, 1);
            switch (show) {
                case 0:
                    break;
                case 1:
                    if (d.getMonth() == 11) {
                        if (row % 2 == 0)
                            html += "<tr style=\"background-color:#FFC; text-align:right\"><td>" + d.getFullYear() + "</td><td>$" + addCommas(YearTotalI.toFixed(0)) + "</td><td>$" + addCommas(YearTotalP.toFixed(0)) + "</td><td>$" + addCommas(adjustedPrincipal.toFixed(0)) + "</td></tr>\n";
                        else
                            html += "<tr style=\"text-align:right\"><td>" + d.getFullYear() + "</td><td>$" + addCommas(YearTotalI.toFixed(0)) + "</td><td>$" + addCommas(YearTotalP.toFixed(0)) + "</td><td>$" + addCommas(adjustedPrincipal.toFixed(0)) + "</td></tr>\n";
                        row++;
                    }
                    break;
                case 2:
                    if (d.getMonth() == 1) {
                        html += "<table width=\"100%\">\n<tr style=\"background-color:blue; color:#FC6; text-align:center\"><th>Payment Month</th><th>Payment</th><th>Interest</th><th>Principal</th><th>Remaining Balance</th></tr>\n";
                    }
                    if (row % 2 == 0)
                        html += "<tr style=\"background-color:#FFC; text-align:right\"><td>" + (d.getMonth() == 0 ? 12 : d.getMonth()) + "/" + d.getDate() + "/" + d.getFullYear() + "</td><td>$" + addCommas(Math.round(adjusted)) + "</td><td>$" + addCommas(MI.toFixed(0)) + "</td><td>$" + addCommas(MP.toFixed(0)) + "</td><td>$" + addCommas(adjustedPrincipal.toFixed(0)) + "</td></tr>\n";
                    else
                        html += "<tr style=\"text-align:right\"><td>" + (d.getMonth() == 0 ? 12 : d.getMonth()) + "/" + d.getDate() + "/" + d.getFullYear() + "</td><td>$" + addCommas(Math.round(adjusted)) + "</td><td>$" + addCommas(MI.toFixed(0)) + "</td><td>$" + addCommas(MP.toFixed(0)) + "</td><td>$" + addCommas(adjustedPrincipal.toFixed(0)) + "</td></tr>\n";
                    
                    if (d.getMonth() == 0 || adjustedPrincipal == 0) {
                        html += "<tr><td colspan=5><center><img src=\"/images/mainpx.gif\" width=\"470px\" height=\"1px\"\"></center></td></tr>\n"
                            + "<tr style=\"color:blue\"><td>" + (d.getFullYear()-1) + " Totals</td><td style=\"text-align:right\">$" + addCommas((12 * adjusted).toFixed(0)) + "</td><td style=\"text-align:right\">$" + addCommas(YearTotalI.toFixed(0)) + "</td><td style=\"text-align:right\">$" + addCommas(YearTotalP.toFixed(0)) + "</td><td style=\"text-align:right\">$" + addCommas(adjustedPrincipal.toFixed(0)) + "</td></tr>\n</table>\n</br>\n<br/>\n";
                    }
                    
                    row++;
            }
            YearTotalI += MI;
            YearTotalP += MP;
        }

        if (show == 1)
            html += "</table>\n";

        document.getElementById('AmortGraphs13').innerHTML = html;
        
    }

    function addCommas(str) {
        return (str + "").replace(/\b(\d+)((\.\d+)*)\b/g, function (a, b, c) {
            return (b.charAt(0) > 0 && !(c || ".").lastIndexOf(".") ? b.replace(/(\d)(?=(\d{3})+$)/g, "$1,") : b) + c;
        });
    }
