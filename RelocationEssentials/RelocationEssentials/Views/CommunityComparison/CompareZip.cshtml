@{
    ViewBag.Title = "CompareZip";
}

<div id="main">
    <img src="/images/icon_commcompare.gif" />
    <img src="/images/title_communitycomparison.gif" />
    <div class="tabbed">
        <img src="/images/bnr_demographics.gif" />
        <br />
        <span style="float:left; width:40%">Location</span>
        <span style="float:left; margin-right:25px; width:25%">@((ViewBag.M1).Location) (@ViewBag.Zip1)</span>
        <span style="float:left; margin-right:25px; width:25%">@((ViewBag.M2).Location) (@ViewBag.Zip2)</span>
        <br />
        <span style="float:left; width:40%">County Seat</span>
        <span style="float:left; margin-right:25px; width:25%">@((ViewBag.M1).CountySeat)</span>
        <span style="float:left; margin-right:25px; width:25%">@((ViewBag.M2).CountySeat)</span>
        <br />
        <span style="float:left; width:40%">Area by Square Miles</span>
        <span style="float:left; margin-right:25px; width:25%">@((ViewBag.M1).Area)</span>
        <span style="float:left; margin-right:25px; width:25%">@((ViewBag.M2).Area)</span>
        <br />
        <span style="float:left; width:40%">Population</span>
        <span style="float:left; margin-right:25px; width:25%">@((ViewBag.M1).Population)</span>
        <span style="float:left; margin-right:25px; width:25%">@((ViewBag.M2).Population)</span>
        <br />
        <span style="float:left; width:40%">Population Density</span>
        <span style="float:left; margin-right:25px; width:25%">@((ViewBag.M1).Density)</span>
        <span style="float:left; margin-right:25px; width:25%">@((ViewBag.M2).Density)</span>
        <br />
        <span style="float:left; width:40%">Median Age</span>
        <span style="float:left; margin-right:25px; width:25%">@((ViewBag.M1).Age)</span>
        <span style="float:left; margin-right:25px; width:25%">@((ViewBag.M2).Age)</span>
        <br />
        <span style="float:left; width:40%">Average Family Size</span>
        <span style="float:left; margin-right:25px; width:25%">@((ViewBag.M1).FamilySize)</span>
        <span style="float:left; margin-right:25px; width:25%">@((ViewBag.M2).FamilySize)</span>
        <br />
        <span style="float:left; width:40%">Total Households</span>
        <span style="float:left; margin-right:25px; width:25%">@((ViewBag.M1).Households)</span>
        <span style="float:left; margin-right:25px; width:25%">@((ViewBag.M2).Households)</span>
        <br />
        <span style="float:left; width:40%">Average Household Size</span>
        <span style="float:left; margin-right:25px; width:25%">@((ViewBag.M1).HouseholdSize)</span>
        <span style="float:left; margin-right:25px; width:25%">@((ViewBag.M2).HouseholdSize)</span>
        <br />
        <span style="float:left; width:40%">Median Home Value</span>
        <span style="float:left; margin-right:25px; width:25%">@((ViewBag.M1).MHV)</span>
        <span style="float:left; margin-right:25px; width:25%">@((ViewBag.M2).MHV)</span>
        <br />
        <img src="/images/bnr_employment.gif" />
        <br />
        <span style="float:left; width:40%">Income per Capita</span>
        <span style="float:left; margin-right:25px; width:25%">@((ViewBag.M1).IC)</span>
        <span style="float:left; margin-right:25px; width:25%">@((ViewBag.M2).IC)</span>
        <br />
        <span style="float:left; width:40%">Median Household Income</span>
        <span style="float:left; margin-right:25px; width:25%">@((ViewBag.M1).MHI)</span>
        <span style="float:left; margin-right:25px; width:25%">@((ViewBag.M2).MHI)</span>
        <br />
        <span style="float:left; width:40%">Unemployment Rate</span>
        <span style="float:left; margin-right:25px; width:25%">@((ViewBag.M1).UR)</span>
        <span style="float:left; margin-right:25px; width:25%">@((ViewBag.M2).UR)</span>
        <br />
        <span style="float:left; width:40%">Commute Time (minutes)</span>
        <span style="float:left; margin-right:25px; width:25%">@((ViewBag.M1).Commute)</span>
        <span style="float:left; margin-right:25px; width:25%">@((ViewBag.M2).Commute)</span>
        <br />
        <img src="/images/bnr_scholastics.gif" />
        <br />
        <span style="float:left; width:40%">High School Graduation</span>
        <span style="float:left; margin-right:25px; width:25%">@((ViewBag.M1).HSG)</span>
        <span style="float:left; margin-right:25px; width:25%">@((ViewBag.M2).HSG)</span>
        <br />
