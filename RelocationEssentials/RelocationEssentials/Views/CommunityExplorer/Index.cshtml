			args.IsValid = true;
		}
		else
		{
			args.IsValid = false;
		}
	}
	
	function select6(oSrc,args)
	{
		d=document.Index;
		if ( d.marital.value == "x" || d.age.value == "x" || d.income.value == "x" || d.houseval.value == "x" || d.owned.value == "x" || d.school.value == "x")
		{
			args.IsValid = false;
		}
		else
		{
			args.IsValid = true;
		}
	}
	
	function StateAndCity(oSrc,args)
	{
		d=document.Index;
		if( d.StateList.value != "0" && d.CityList.value == 0)
		{
			args.IsValid = false;
		}
		else
		{
			args.IsValid = true;
		}
	}
</script>
